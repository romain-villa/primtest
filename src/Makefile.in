SHELL          = /bin/sh

CC        = @CC@
CFLAGS    = @CFLAGS@
LDFLAGS   = @LDFLAGS@
OBJS      = @OBJS@

.PHONY: primtest clean

primtest: $(OBJS)
	$(CC) -o $@ $(OBJS) -g $(LDFLAGS)

primtest.o: main.c *.h
	$(CC) $(CFLAGS) $< 

%.o: %.c %.h
	$(CC) $(CFLAGS) $< 

DOSSIER = IN603_AMMAD_Massina_VILLA_Romain
CF = config

clean:
	rm -f $(DOSSIER).zip
	rm -f $(OBJS)
	rm -f primtest
	ls -l
